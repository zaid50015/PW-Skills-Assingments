    "# 4. Check how many times a given number can be divided by 3 before it is less than or equal to 10\n",
    "num=int(input(\"Enter a number\"))\n",
    "count=0;\n",
    "while num>10:\n",
    "    num/=3\n",
    "    count+=1\n",
    "print(count)"
